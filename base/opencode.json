{
  "$schema": "https://opencode.ai/config.json",
  "model": "parasail/parasail-minimax-m25",
  "mode": {
    "plan": {
      "model": "parasail/parasail-minimax-m25"
    },
    "build": {
      "model": "parasail/parasail-minimax-m25"
    }
  },
  "provider": {
    "parasail": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Parasail",
      "options": {
        "baseURL": "https://api-webflux.saas.parasail.io/v1",
        "apiKey": "{env:PARASAIL_API_KEY}"
      },
      "models": {
        "parasail-glm-5": {
          "name": "GLM-5"
        },
        "parasail-glm47": {
          "name": "GLM-4.7"
        },
        "parasail-glm-46v": {
          "name": "GLM-4.6V",
          "attachment": true
        },
        "parasail-minimax-m25": {
          "name": "MiniMax M2.5"
        },
        "parasail-deepseek-v32": {
          "name": "DeepSeek V3.2"
        },
        "parasail-qwen3-235b-a22b-instruct-2507": {
          "name": "Qwen3 235B Instruct"
        },
        "parasail-kimi-k25": {
          "name": "Kimi K2.5 (262k ctx)"
        },
        "parasail-kimi-k2-instruct": {
          "name": "Kimi K2 (262k ctx)"
        },
        "parasail-kimi-k2-thinking": {
          "name": "Kimi K2 (262k ctx)"
        },
        "parasail-mistral-small-32-24b": {
          "name": "Mistral Small (Fast)"
        }
      }
    }
  },
  "mcp": {
    "context7": {
      "type": "remote",
      "url": "https://mcp.context7.com/mcp",
      "headers": {
        "CONTEXT7_API_KEY": "{env:CONTEXT7_API_KEY}"
      },
      "enabled": true
    }
  }
}